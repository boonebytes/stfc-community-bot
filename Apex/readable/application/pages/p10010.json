       ,"Language":"PL\/SQL"
       ,"PL\/SQL Code":"begin\n    if :P10010_ALLOW_OTHER_USERS = \u0027Y\u0027 then\n        apex_app_setting.set_value (\n            p_name  =\u003E \u0027ACCESS_CONTROL_SCOPE\u0027,\n            p_value =\u003E \u0027ALL_USERS\u0027);\n    else\n        apex_app_setting.set_value (\n            p_name  =\u003E \u0027ACCESS_CONTROL_SCOPE\u0027,\n            p_value =\u003E \u0027ACL_ONLY\u0027);\n    end if;\nend;"
      }
